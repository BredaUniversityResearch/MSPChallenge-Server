{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": 
    {
        "ext-ctype": "*",
        "ext-curl": "*",
        "ext-iconv": "*",
        "ext-imagick": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-openssl": "*",
        "ext-pdo": "*",
        "ext-sodium": "*",
        "ext-zip": "*",
        "blackfire/php-sdk": "^1.33",
        "cboden/ratchet": "^0.4",
        "clue/http-proxy-react": "^1.8",
        "doctrine/doctrine-bundle": "^2.6",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.12",
        "drift/dbal": "dev-master",
        "gasparesganga/php-shapefile": "3.3.3",
        "illuminate/collections": "^8.73.2",
        "jfcherng/php-diff": "*",
        "justinrainbow/json-schema": "*",
        "lcobucci/jwt": "^4.0",
        "lexik/jwt-authentication-bundle": "*",
        "phpdocumentor/reflection-docblock": "^5.3",
        "phpstan/phpdoc-parser": "^1.25",
        "predis/predis": "^2.2",
        "react/http": "^1.5",
        "react/mysql": "^0.5.6",
        "react/promise": "2.10.*",
        "shivas/versioning-bundle": "*",
        "symfony/console": "5.4.*",
        "symfony/doctrine-messenger": "5.4.*",
        "symfony/dotenv": "5.4.*",
        "symfony/finder": "5.4.*",
        "symfony/flex": "^1.3.1",
        "symfony/form": "5.4.*",
        "symfony/framework-bundle": "5.4.*",
        "symfony/http-client": "5.4.*",
        "symfony/messenger": "5.4.*",
        "symfony/monolog-bundle": "^3.8",
        "symfony/property-access": "5.4.*",
        "symfony/property-info": "5.4.*",
        "symfony/proxy-manager-bridge": "5.4.*",
        "symfony/runtime": "5.4.*",
        "symfony/security-bundle": "5.4.*",
        "symfony/serializer": "5.4.*",
        "symfony/translation": "5.4.*",
        "symfony/twig-bundle": "5.4.*",
        "symfony/uid": "5.4.*",
        "symfony/validator": "5.4.*",
        "symfony/yaml": "5.4.*"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "symfony/runtime": true,
            "phpstan/extension-installer": true
        },
        "platform": {
            "php": "8.1"
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": [
                "src/",
                "src-symfony5.4/"
            ],
            "App\\Domain\\API\\": "api",
            "DoctrineMigrations\\": "migrations/src/",
            "ServerManager\\": "ServerManager/classes/"
        },
        "files": [
            "src/helpers.php",
            "src/functions.php",
            "src/Domain/WsServer/functions.php",
            "ServerManager/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/",
	        "App\\DataFixtures\\": "fixtures/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts",
            "bash tools/cghooks-fresh-install.sh"
        ],
        "post-update-cmd": [
            "@auto-scripts",
            "bash tools/cghooks-fresh-install.sh"
        ],
        "cghooks": "vendor/bin/cghooks"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "hooks": {
            "config": {
                "stop-on-failure": ["pre-push"]
            },
            "post-merge": [
                "bash install.sh"
            ],
            "pre-push": [
                "bash test.sh",
                "vendor/bin/phpstan analyse"
            ]
        },
        "symfony": {
            "allow-contrib": false,
            "require": "5.4.*"
        }
    },
    "require-dev": {
        "brainmaestro/composer-git-hooks": "dev-master#a714e58968a37dba2783417a5d26ef4b1afd8268",
        "doctrine/doctrine-fixtures-bundle": "*",
        "jetbrains/phpstorm-attributes": "^1.0",
        "phpstan/extension-installer": "^1.2",
        "phpstan/phpstan": "^1.9",
        "phpstan/phpstan-doctrine": "^1.3",
        "phpstan/phpstan-symfony": "^1.2",
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "3.*",
        "symfony/browser-kit": "5.4.*",
        "symfony/css-selector": "5.4.*",
        "symfony/maker-bundle": "^1.43",
        "symfony/phpunit-bridge": "^6.3",
        "symfony/stopwatch": "5.4.*",
        "symfony/web-profiler-bundle": "5.4.*"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:BredaUniversityResearch/composer-git-hooks.git"
        }
    ]
}
