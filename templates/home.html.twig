{% extends 'base.html.twig' %}

{% block title %}MSP Challenge Server{% endblock %}
{% block subtitle %}{% endblock %}
{% block menuUser %}{% endblock %}
{% block body %}
    <main>
        <section>
            <h2>Links</h2>
            <hr>
            <ul>
                <li><a href="{{ path('server_manager_index') }}">Go to the Server Manager</a></li>
            </ul>
        </section>
        {% if downloads is not empty %}
        <section>
            <h2>Available Downloads</h2>
            <hr>
            <ul>
                {% for download in downloads %}
                    <li><a href="{{ asset('downloads/' ~ download) }}">{{ download }}</a></li>
                {% endfor %}
            </ul>
        </section>
        {% endif %}
    </main>
{% endblock %}
