{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "suggest": {
        "ext-imagick": "You only require imagick for image processing, e.g. for POV config creation."
    },
    "require":
    {
        "php": ">=8.2",
        "ext-ctype": "*",
        "ext-curl": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-openssl": "*",
        "ext-pdo": "*",
        "ext-sodium": "*",
        "ext-zip": "*",
        "blackfire/php-sdk": "^1.33",
        "cboden/ratchet": "^0.4",
        "clue/http-proxy-react": "^1.8",
        "doctrine/annotations": "^2.0",
        "doctrine/doctrine-bundle": "^2.11",
        "doctrine/doctrine-fixtures-bundle": "*",
        "doctrine/doctrine-migrations-bundle": "^3.3",
        "doctrine/orm": "^2.18",
        "drift/dbal": "dev-master",
        "dunglas/doctrine-json-odm": "^1.4",
        "gasparesganga/php-shapefile": "3.3.3",
        "illuminate/collections": "^8.73.2",
        "jfcherng/php-diff": "^6.15",
        "justinrainbow/json-schema": "*",
        "knplabs/knp-menu-bundle": "^3.2",
        "lcobucci/jwt": "^4.0",
        "lexik/jwt-authentication-bundle": "^2.20",
        "phpdocumentor/reflection-docblock": "^5.3",
        "phpstan/phpdoc-parser": "^1.25",
        "phpunit/phpunit": "^9.5",
        "predis/predis": "^2.2",
        "react/http": "^1.5",
        "react/mysql": "^0.5.6",
        "react/promise": "2.10.*",
        "runtime/frankenphp-symfony": "^0.2.0",
        "shivas/versioning-bundle": "^4.0",
        "swaggest/json-schema": "^0.12.42",
        "symfony/asset": "6.4.*",
        "symfony/browser-kit": "6.4.*",
        "symfony/console": "6.4.*",
        "symfony/css-selector": "6.4.*",
        "symfony/doctrine-messenger": "6.4.*",
        "symfony/dotenv": "6.4.*",
        "symfony/expression-language": "6.4.*",
        "symfony/filesystem": "6.4.*",
        "symfony/finder": "6.4.*",
        "symfony/flex": "^2",
        "symfony/form": "6.4.*",
        "symfony/framework-bundle": "6.4.*",
        "symfony/http-client": "6.4.*",
        "symfony/intl": "6.4.*",
        "symfony/mailer": "6.4.*",
        "symfony/messenger": "6.4.*",
        "symfony/mime": "6.4.*",
        "symfony/monolog-bundle": "^3.8",
        "symfony/notifier": "6.4.*",
        "symfony/phpunit-bridge": "^7.0",
        "symfony/process": "6.4.*",
        "symfony/property-access": "6.4.*",
        "symfony/property-info": "6.4.*",
        "symfony/proxy-manager-bridge": "6.4.*",
        "symfony/runtime": "6.4.*",
        "symfony/security-bundle": "6.4.*",
        "symfony/serializer": "6.4.*",
        "symfony/stimulus-bundle": "^2.22",
        "symfony/string": "6.4.*",
        "symfony/translation": "6.4.*",
        "symfony/twig-bundle": "6.4.*",
        "symfony/uid": "6.4.*",
        "symfony/ux-turbo": "^2.22",
        "symfony/validator": "6.4.*",
        "symfony/web-link": "6.4.*",
        "symfony/webpack-encore-bundle": "^2.2",
        "symfony/yaml": "6.4.*",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^2.12|^3.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "allow-plugins": {
            "php-http/discovery": true,
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "symfony/runtime": true,
            "phpstan/extension-installer": true
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": [
                "src/",
                "src-symfony6.4/"
            ],
            "App\\Domain\\API\\": "api",
            "DoctrineMigrations\\": "migrations/src/",
            "ServerManager\\": "ServerManager/classes/",
            "App\\Tests\\": "tests/",
            "App\\DataFixtures\\": "fixtures/"
        },
        "files": [
            "src/helpers.php",
            "src/functions.php",
            "src/Domain/WsServer/functions.php",
            "ServerManager/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/",
	        "App\\DataFixtures\\": "fixtures/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd"
        },
        "post-install-cmd": [
            "(php bin/console doctrine:cache:clear-metadata -n || true)",
            "@auto-scripts",
            "bash tools/cghooks-fresh-install.sh"
        ],
        "post-update-cmd": [
            "(php bin/console doctrine:cache:clear-metadata -n || true)",
            "@auto-scripts",
            "bash tools/cghooks-fresh-install.sh"
        ],
        "cghooks": "vendor/bin/cghooks"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "hooks": {
            "config": {
                "stop-on-failure": ["pre-push"]
            },
            "post-merge": [
                "bash install.sh"
            ],
            "pre-push": [
                "bash test.sh",
                "vendor/bin/phpstan analyse --memory-limit=1G"
            ]
        },
        "symfony": {
            "allow-contrib": false,
            "require": "6.4.*"
        }
    },
    "require-dev": {
        "brainmaestro/composer-git-hooks": "dev-master#a714e58968a37dba2783417a5d26ef4b1afd8268",
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "jetbrains/phpstorm-attributes": "^1.0",
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpstan": "1.12.0",
        "phpstan/phpstan-deprecation-rules": "^1.1",
        "phpstan/phpstan-doctrine": "^1.3",
        "phpstan/phpstan-symfony": "^1.2",
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "3.*",
        "symfony/browser-kit": "6.4.*",
        "symfony/css-selector": "6.4.*",
        "symfony/maker-bundle": "^1.43",
        "symfony/phpunit-bridge": "^7.0",
        "symfony/stopwatch": "6.4.*",
        "symfony/web-profiler-bundle": "6.4.*"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:BredaUniversityResearch/composer-git-hooks.git"
        }
    ]    
}
