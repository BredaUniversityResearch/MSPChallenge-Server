{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "suggest": {
        "ext-imagick": "You only require imagick for image processing, e.g. for POV config creation."
    },
    "require": {
        "php": ">=8.3",
        "ext-ctype": "*",
        "ext-curl": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-openssl": "*",
        "ext-pdo": "*",
        "ext-sodium": "*",
        "ext-zip": "*",
        "api-platform/doctrine-orm": "^4.1",
        "api-platform/symfony": "^4.1",
        "blackfire/php-sdk": "^2.5",
        "cboden/ratchet": "dev-master",
        "clue/http-proxy-react": "^1.9",
        "doctrine/annotations": "^2.0",
        "doctrine/dbal": "^3",
        "doctrine/doctrine-bundle": "^2.13",
        "doctrine/doctrine-fixtures-bundle": "^4.0",
        "doctrine/doctrine-migrations-bundle": "^3.3",
        "doctrine/orm": "^3.3",
        "drift/dbal": "dev-master",
        "dunglas/doctrine-json-odm": "^1.4",
        "gasparesganga/php-shapefile": "^3.4",
        "illuminate/collections": "^11.37",
        "jfcherng/php-diff": "^6.16",
        "justinrainbow/json-schema": "^6.0",
        "knplabs/knp-menu-bundle": "^3.5",
        "lcobucci/jwt": "^5.5",
        "lexik/jwt-authentication-bundle": "^3.1",
        "nelmio/api-doc-bundle": "^5.6",
        "nelmio/cors-bundle": "^2.5",
        "phpdocumentor/reflection-docblock": "^5.6",
        "phpstan/phpdoc-parser": "^2.0",
        "phpunit/phpunit": "^9.5",
        "predis/predis": "^2.3",
        "react/http": "^1.5",
        "react/mysql": "^0.6.0",
        "react/promise": "^3.3",
        "shivas/versioning-bundle": "^4.1",
        "stof/doctrine-extensions-bundle": "^1.14",
        "swaggest/json-schema": "^0.12.43",
        "symfony/asset": "*",
        "symfony/asset-mapper": "*",
        "symfony/browser-kit": "*",
        "symfony/console": "*",
        "symfony/css-selector": "*",
        "symfony/doctrine-messenger": "*",
        "symfony/dotenv": "*",
        "symfony/expression-language": "*",
        "symfony/filesystem": "*",
        "symfony/finder": "*",
        "symfony/flex": "^2",
        "symfony/form": "*",
        "symfony/framework-bundle": "*",
        "symfony/http-client": "*",
        "symfony/intl": "*",
        "symfony/mailer": "*",
        "symfony/messenger": "*",
        "symfony/mime": "*",
        "symfony/monolog-bundle": "^3.0",
        "symfony/notifier": "*",
        "symfony/phpunit-bridge": "^7.4",
        "symfony/process": "*",
        "symfony/property-access": "*",
        "symfony/property-info": "*",
        "symfony/runtime": "*",
        "symfony/security-bundle": "*",
        "symfony/serializer": "*",
        "symfony/stimulus-bundle": "^2.22",
        "symfony/string": "*",
        "symfony/translation": "*",
        "symfony/twig-bundle": "*",
        "symfony/uid": "*",
        "symfony/ux-turbo": "^2.22",
        "symfony/validator": "*",
        "symfony/web-link": "*",
        "symfony/webpack-encore-bundle": "^2.2",
        "symfony/yaml": "*",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^2.12|^3.0"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true,
            "phpstan/extension-installer": true
        },
        "bump-after-update": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": [
                "src/"
            ],
            "App\\Domain\\API\\": "api",
            "DoctrineMigrations\\": "migrations/src/",
            "ServerManager\\": "ServerManager/classes/",
            "App\\Tests\\": "tests/",
            "App\\DataFixtures\\": "fixtures/"
        },
        "files": [
            "src/helpers.php",
            "src/functions.php",
            "src/Domain/WsServer/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/",
	        "App\\DataFixtures\\": "fixtures/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd"
        },
        "post-install-cmd": [
            "(php bin/console doctrine:cache:clear-metadata -n || true)",
            "@auto-scripts",
            "bash tools/cghooks-fresh-install.sh"
        ],
        "post-update-cmd": [
            "(php bin/console doctrine:cache:clear-metadata -n || true)",
            "@auto-scripts",
            "bash tools/cghooks-fresh-install.sh"
        ],
        "cghooks": "vendor/bin/cghooks"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "hooks": {
            "config": {
                "stop-on-failure": ["pre-push"]
            },
            "post-merge": [
                "bash install.sh"
            ],
            "pre-push": [
                "bash test.sh",
                "vendor/bin/phpstan analyse --memory-limit=1G"
            ]
        },
        "symfony": {
            "allow-contrib": false,
            "require": "7.4.*"
        }
    },
    "require-dev": {
        "brainmaestro/composer-git-hooks": "dev-master",
        "doctrine/doctrine-fixtures-bundle": "^4.0",
        "jetbrains/phpstorm-attributes": "^1.0",
        "phpstan/extension-installer": "^1.4",
        "phpstan/phpstan": "^2.1",
        "phpstan/phpstan-deprecation-rules": "^2.0",
        "phpstan/phpstan-doctrine": "^2.0",
        "phpstan/phpstan-symfony": "^2.0",
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.11",
        "symfony/browser-kit": "*",
        "symfony/css-selector": "*",
        "symfony/debug-bundle": "*",
        "symfony/maker-bundle": "^1.0",
        "symfony/phpunit-bridge": "^7.4",
        "symfony/stopwatch": "*",
        "symfony/web-profiler-bundle": "*"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/BredaUniversityResearch/composer-git-hooks.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/BredaUniversityResearch/Ratchet.git"
        }
    ]
}
